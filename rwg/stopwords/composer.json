{
	"name": "rwg/stopwords",
	"type": "phpbb-extension",
	"description": "Prevent common words (stopwords) from using space in the native search index",
	"version": "0.1.0",
	"time": "2022-02-23",
	"license": "GPL-2.0",
	"authors": [{
			"name": "Robin Glover",
			"homepage": "https://sharpcap.co.uk"
		}],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Exclude stopwords from native search",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0"
		}
	}
}
